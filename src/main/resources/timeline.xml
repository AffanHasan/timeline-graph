<?xml version="1.0" encoding="UTF-8"?>

<graphml xmlns="http://graphml.graphdrawing.org/xmlns"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns
     http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">
    <!-- Attributes -->
    <key id='labelV'  for='node' attr.name='labelV'  attr.type='string'></key>
    <key id='labelE'  for='edge' attr.name='labelE'  attr.type='string'></key>
    <key id='type'    for='node' attr.name='type'    attr.type='string'></key>
    <key id='yearValue' for='node' attr.name='value' attr.type='int'></key>
    <key id='name'    for='node' attr.name='name'    attr.type='string'></key>
    <key id='source' for='edge' attr.name='source' attr.type='string'></key>
    <key id='reference' for='edge' attr.name='reference' attr.type='string'></key>
    <key id='era' for='vertex' attr.name='era' attr.type='string'></key>

    <graph id='project-timeline' edgedefault='directed'>
        <!-- Nodes -->
        <node id='0'>
            <data key='labelV'>version</data>
            <data key='type'>version</data>
            <data key='code'>0.1</data>
            <data key='desc'>Version: 0.1 Generated: May-18-2023 Graph created by
                Affan Hasan</data>
        </node>
        <!-- Timelines Graph -->
        <node id='1'>
            <data key='labelV'>Graph</data>
            <data key='name'>Timeline</data>
        </node>
        <node id='2'>
            <data key='labelV'>Timeline</data>
            <data key='name'>Hijri</data>
        </node>
        <node id='3'>
            <data key='labelV'>Timeline</data>
            <data key='name'>Gregorian</data>
        </node>
        <node id='8'>
            <data key='labelV'>Before-Hijri</data>
            <data key='name'>B.H</data>
        </node>
        <node id='9'>
            <data key='labelV'>After-Hijri</data>
            <data key='name'>A.H</data>
        </node>
        <node id='12'>
            <data key='labelV'>Year</data>
            <data key='value'>53</data>
        </node>
        <!-- Events -->
        <node id='4'>
            <data key='labelV'>Event</data>
            <data key='type'>Death</data>
            <data key='name'>Birth Of Last Prophet Muhammad S.A.W</data>
        </node>
        <node id='5'>
            <data key='labelV'>Event</data>
            <data key='type'>Birth</data>
            <data key='name'>Birth Of Last Prophet Muhammad S.A.W</data>
        </node>
        <node id='14'>
            <data key='labelV'>Prophet</data>
            <data key='name'>Muhammad Bin Abdullah</data>
        </node>
        <!-- Edges -->
        <edge id='6' source='1' target='2'>
            <data key='labelE'>type</data>
        </edge>
        <edge id='7' source='1' target='3'>
            <data key='labelE'>type</data>
        </edge>
        <edge id='10' source='2' target='8'>
            <data key='labelE'>before</data>
        </edge>
        <edge id='11' source='2' target='9'>
            <data key='labelE'>after</data>
        </edge>
        <edge id='13' source='8' target='12'>
            <data key='labelE'>year</data>
        </edge>
        <edge id='15' source='14' target='12'>
            <data key='labelE'>borned_in</data>
        </edge>
